<rules
    xmlns="http://namespaces.plone.org/diazo"
    xmlns:css="http://namespaces.plone.org/diazo/css"
    xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
    >

    <replace
        css:theme="#alpha"
        css:content="#one"
        />

    <replace
        css:theme="#beta"
        css:content="#two"
        />

    <strip
        css:content="#one"
        css:if-content="#two"
        />

    <strip
        css:content="#two"
        css:if-content="#three"
        />


</rules>
